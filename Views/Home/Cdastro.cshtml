@{
    ViewData["Title"] = "Cadastro";
}


<head>
    <link rel="stylesheet" href="~/css/style.css" />
</head>

<nav class="navbar navbar-expand-lg navbar-dark navbar-custom static-top">
    <div class="container">
        <a class="navbar-brand" asp-action="Index" asp-controller="Home"><img src="~/img/logo_nova.png" alt="logo Favela Delivery" height="70"> </a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav ms-auto navcolor">
                <li class="nav-item">
                    <a class="nav-link" aria-current="page" asp-action="Index" asp-controller="Home">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-action="Quemsomos" asp-controller="Home">Quem somos</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-action="Pedidos" asp-controller="Home">Pedidos</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-action="Cdastro" asp-controller="Home">Cadastro</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-action="Contato" asp-controller="Home">Contato</a>
                </li>
                <div class="icons">
                    <a href="https://www.instagram.com/favela_delivery/" target="_blank"><i class="bi bi-instagram"></i></a>
                    <a href="https://www.facebook.com/" target="_blank"><i class="bi bi-facebook"></i></a>
                </div>
            </ul>
        </div>
    </div>
</nav>

<section class="cadastro">
    <h2>Cadastro</h2>
    <p>
        Escolha seu segmento e faça seu cadastro.
    </p>
    <div class="box-cadastro">
        <div class="box-cadastro-img">
            <img src="/img/ft-empreendedor.png" class="alignnone" alt="" width="200" height="150" />
            <h4>Empreendedor</h4>
            <a class="nav-link text-dark" asp-area="" asp-controller="CadastroEmpreendedors" asp-action="Create"><button class="btn" type="submit">Cadastrar</button></a>
        </div>
        <div  class="box-cadastro-img">
            <img src="/img/ft-entregador.png" class="alignnone" alt="" width="200" height="150" />
            <h4>Entregador</h4>
            <a class="nav-link text-dark" asp-area="" asp-controller="CadastroEntregadors" asp-action="Create"><button class="btn" type="submit">Cadastrar</button></a>
        </div>
        <div  class="box-cadastro-img">
            <img src="/img/ft-cliente.png" class="alignnone" alt="" width="200" height="150" />
            <h4>Cliente</h4>
            <a class="nav-link text-dark" asp-area="" asp-controller="CadastroClientes" asp-action="Create"><button class="btn" type="submit">Cadastrar</button></a>
        </div>
    </div>
</section>

